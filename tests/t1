def int[] makeArray(int size){
    int[size] array;
    return array;
}

def int[] fibb(int[] array,int ammount){
    array[0] = 1;
    array[1] = 1;
    for(int i=2; i<ammount; i+=1;){
        int val = 0;
        int j = val;
        array[i] = array[i-1] + array[i-2] + val;
    }
    return array;
}
